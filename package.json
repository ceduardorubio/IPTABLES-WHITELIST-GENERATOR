{
    "name": "ip-country-filter-list",
    "version": "1.0.0",
    "description": "Use this script to easily allow request only from the countries you choose.",
    "main": "index.js",
    "scripts": {
        "download": "wget https://download.ip2location.com/lite/IP2LOCATION-LITE-DB1.CSV.ZIP;",
        "unzip": "unzip IP2LOCATION-LITE-DB1.CSV.ZIP -d ./DB;",
        "remove_zip": "rm IP2LOCATION-LITE-DB1.CSV.ZIP;",
        "start": "npm run download; npm run unzip; npm run remove_zip; node index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ceduardorubio/WHITELISTS-BY-COUNTRIES.git"
    },
